<section class="section">
  <div class="container">
    <div class="row">
      <div class="col-3">
        <% if user_signed_in? %>
          <%= render 'profile' %>
        <% else %>
          <%= render 'trends' %>
        <% end %>
      </div>
      <div class="col-6">
        <%= render 'feed' %>
      </div>
      <div class="col-3">
        <%= render 'posters' %>
      </div>
    </div>
  </div>
</section>
